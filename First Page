namespace Fitness_Tracker_Application
{
    public partial class FirstPage : Form
    {
        public FirstPage()
        {
            InitializeComponent();
        }

        private void label3_Click(object sender, EventArgs e)
        {

        }

        private void lblAdmin_LinkClicked(object sender, LinkLabelLinkClickedEventArgs e)
        {
            AdminLogin AL = new AdminLogin();
            AL.Show();
            this.Hide();
        }

        private void lblMember_LinkClicked(object sender, LinkLabelLinkClickedEventArgs e)
        {
            WelcomePage WC = new WelcomePage();
            WC.Show();
            this .Hide();
        }
    }
